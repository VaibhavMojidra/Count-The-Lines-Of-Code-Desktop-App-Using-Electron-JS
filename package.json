{
  "name": "com.vaibhavmojidra.countthelinesofcode",
  "productName": "Count The Lines Of Code",
  "version": "1.0.0",
  "description": "Vaibhav Mojidra",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/assets/icons/mac/icon.icns --asar --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=\"Vaibhav Mojidra\" --version-string.FileDescription=\"Vaibhav Mojidra Apps - Count The Lines Of Code\" --version-string.ProductName=\"Count The Lines Of Code\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=app/assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [
    "Vaibhav",
    "Mojidra",
    "Count",
    "The",
    "Lines",
    "Of",
    "Code"
  ],
  "author": "Vaibhav Mojidra",
  "license": "MIT",
  "devDependencies": {
    "electron": "^20.1.0",
    "electron-packager": "^16.0.0"
  }
}